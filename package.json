{
  "name": "dev-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "^5.1.1",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "download-schema": "apollo client:download-schema ./src/graphql-schema.json",
    "generate-dts-queries": "find src -depth -name __generated__ -exec rm -rf {} \\; ; apollo client:codegen __generated__ --globalTypesFile=./src/__generated__/globalTypes.ts --localSchemaFile=./src/graphql-schema.json --target=typescript --includes='./src/**/apollo/**/*.ts' --tagName=gql"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@types/styled-components": "^5.1.2",
    "apollo": "^2.30.3"
  }
}
